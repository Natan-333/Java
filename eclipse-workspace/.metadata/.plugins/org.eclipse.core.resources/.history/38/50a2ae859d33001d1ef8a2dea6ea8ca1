package entrada_processamneto_saida;

public class EX34W {

	public static void main1(String[] args) {
		public static void main(String[] args) {
			int index, result;
			index = 1;
			while (index < 11) {
				result = 5 * index;
				System.out.printf("\n%d X %d = %d", 5, index, result);
				index = index + 1;
			}

		}

	}